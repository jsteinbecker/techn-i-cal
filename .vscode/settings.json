{
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        "**/__pycache__": true
    },
    "explorerExclude.backup": {},
    "python.linting.enabled": false,
    "editor.tabCompletion": "on",
    "diffEditor.codeLens": true,
